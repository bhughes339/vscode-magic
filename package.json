{
    "name": "magic",
    "displayName": "MAGIC",
    "description": "MAGIC support for VSCode",
    "version": "1.4.9",
    "publisher": "bhughes339",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Programming Languages",
        "Other"
    ],
    "activationEvents": [
        "onLanguage:magic"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "languages": [
            {
                "id": "magic",
                "aliases": [
                    "MAGIC",
                    "magic"
                ],
                "extensions": [
                    ".npr-logic"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "ams",
                "aliases": [
                    "AMS"
                ]
            }
        ],
        "grammars": [
            {
                "language": "magic",
                "scopeName": "source.magic",
                "path": "./syntaxes/magic.tmLanguage.json"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "MAGIC configuration",
            "properties": {
                "magic.hoverSeconds": {
                    "type": "boolean",
                    "default": "true",
                    "description": "Hovering over an S(0) value will display its date/time values."
                },
                "magic.hoverVisitNodes": {
                    "type": "boolean",
                    "default": "true",
                    "description": "Hovering over an MRI visit node date string will convert it to a real date."
                }
            }
        },
        "configurationDefaults": {
            "[magic]": {
                "editor.tabSize": 1,
                "editor.detectIndentation": false,
                "editor.quickSuggestions": {
                    "other": false,
                    "comments": false,
                    "strings": false
                },
                "editor.suggestOnTriggerCharacters": false,
                "editor.renderIndentGuides": false
            },
            "[ams]": {
                "editor.rulers": [
                    80
                ],
                "editor.tabSize": 2,
                "editor.insertSpaces": true,
                "editor.wordWrap": "wordWrapColumn",
                "editor.wordWrapColumn": 80,
                "editor.wrappingIndent": "none"
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bhughes339/vscode-magic.git"
    },
    "__metadata": {
        "id": "2002f536-bd69-496b-a3d3-46fe3e438aeb",
        "publisherId": "c2a9ecf3-b739-435d-91d8-9dbbbeb51325",
        "publisherDisplayName": "bhughes339"
    },
    "dependencies": {
        "@types/node": "^8.0.31"
    }
}
